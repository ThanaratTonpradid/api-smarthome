{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.insertFinalNewline": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "vscode-coverage-highlighter.coveredColor": "",
  "typescript.preferences.importModuleSpecifier": "relative",
  "cSpell.ignorePaths": [
    ".git",
    ".vscode",
    "dist",
    "coverage",
    "node_modules",
    "vscode-extension",
    "package-lock.json",
    "package.json",
    "yarn.lock"
  ],
  "files.associations": {
    "*.Makefile": "makefile",
    "*.env*": "dotenv"
  },
  "cSpell.words": [
    "CALLCENTER",
    "CIMB",
    "Fastify",
    "IBANK",
    "ICBC",
    "KBANK",
    "LINEID",
    "Smartking",
    "TBANK",
    "TISCO",
    "TOPUP",
    "bitnami",
    "cors",
    "digitalocean",
    "doks",
    "fullname",
    "kubectl",
    "kubernetes",
    "liff",
    "mget",
    "nestjs",
    "pincode",
    "setex",
    "setnx",
    "stjs",
    "unmock"
  ]
}
